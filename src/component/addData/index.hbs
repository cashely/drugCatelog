<a href="javascript:void(0)" class="goback"><i class="icon-arrow-left"></i>返回</a>
<div class="add-search-than">
    {{#each input}}
        <lable class="input-group">
            {{this.text}}
            <input type="text" class="input {{this.inputClass}}">
        </lable>
    {{/each}}
    <a href="javaScript:void(0)" class="btn find-add-than">查找</a>
</div>
<div class="add-than">
    <div class="than-thead">
        <table class="table">
            <tr class="thead">
                {{#each thead}}
                    <td><div class="table-text">{{this}}</div></td>
                {{/each}}
            </tr>
        </table>
    </div>
    <div class="than-tbody add-than-tbody">
        <table class="table">
            {{#each tbody}}
                <tr data-id="{{id}}">
                    <td><div class="table-text codePro">{{codePro}}</div></td>
                    <td><div class="table-text name">{{name5}}</div></td>
                    <td><div class="table-text">{{flmc}}</div></td>
                    <td><div class="table-text spec">{{spec}}</div></td>
                    <td><div class="table-text manufacturerName">{{manufacturerName}}</div></td>
                    <td><div class="table-text pzwh">{{pzwh}}</div></td>
                    <td><div class="table-text add-than-info">新增</div></td>
                </tr>
            {{/each}}
        </table>
    </div>
</div>