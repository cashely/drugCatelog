<div class="table-diff">
    <div class="table-diff-content">
        <div class="table-diff-bar">
            <a href="javascript:void(0)" class="showThan">比对</a>|<a href="javascript:void(0)" id="showDetail">查看详情</a>|<a href="javascript:void(0)" class="cancel-than">取消比对</a>
        </div>
        <div class="table-diff-header">
            <div class="table-diff-left">
                <div class="table-diff-header">
                    <table width="100%" cellspacing="1" border="0">
                        <thead>
                            {{#each tLeftHead}}
                                <th><div>{{this}}</div></th>
                            {{/each}}
                        </thead>
                    </table>
                </div>
                <div class="table-diff-data">
                    <div class="table-diff-data-content">
                        <table width="100%" cellspacing="1" border="0">
                            <tbody>
                                {{#each ydata}}
                                    <tr data-id={{id}}>
                                        <td class="table-diff-drugId"><div>{{id}}</div></td>
                                        <td><div>{{drugName}}</div></td>
                                        <td><div>{{specName}} {{manufacturerName}}</div></td>
                                        <td class="convertRatio"><div><input value={{convertRatio}}></div></td>
                                        <td class="icon-triangle ymzzy"><div>{{unitName}}</div></td>
                                        <td><div>{{isStop}}</div></td>
                                        <td><div>{{updateDate}}</div></td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="table-diff-right">
                <div class="table-diff-right-all active">
                    <div class="table-diff-header">
                        <div class="table-diff-header-content">
                            <table cellspacing="1" border="0">
                                <thead>
                                    <tr>
                                    {{#each tRightHead}}
                                        <th><div>{{this}}</div></th>
                                    {{/each}}
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                    <div class="table-diff-data">
                        <table cellspacing="1" border="0">
                            <tbody>
                                {{#each ydata}}
                                    <tr>
                                        <td><div>{{codePro}}</div></td>
                                        <td class="icon-triangle"><div>{{ypType}}</div></td>
                                        <td><div>{{proName}}</div></td>
                                        <td><div>{{prepName}}</div></td>
                                        <td><div>{{spec}}</div></td>
                                        <td><div>{{fConvertRatio}}</div></td>
                                        <td><div>{{facAbbrF}}</div></td>
                                        <td><div>{{pzwh}}</div></td>
                                        <td><div>{{flag4}}</div></td>
                                        <td><div>{{jkyw}}</div></td>
                                        <td><div>{{yyyy}}</div></td>
                                        <td><div>{{isZcyzsj}}</div></td>
                                        <td><div>{{jmdf}}</div></td>
                                        <td><div>{{pregnancyLevel}}</div></td>
                                        <td><div>{{flag1}}</div></td>
                                        <td><div>{{ddd}}</div></td>
                                        <td><div>{{adminRouteExclude}}</div></td>
                                        <td><div>{{isCalcKjyw}}</div></td>
                                        <td><div>{{kjywType}}</div></td>
                                        <td><div>{{rbflName}}</div></td>
                                        <td><div>{{ylflName}}</div></td>
                                        <td><div>{{flag5}}</div></td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="table-diff-right-single">
                    <h2 class="table-diff-single-title">标准数据</h2>
                    <div class="table-diff-single-content">
                        <div class="table-details-content-box">
                            <!--标准数据详情-->
                        </div>
                        <div class="tool">
                            <a href="javaScript:void(0)" class="btn-toggle"><i class="icon-arrow-left"></i>切换</a>
                            <a href="javaScript:void(0)" class="btn btn-cancel">取消比对</a>
                            <a href="javaScript:void(0)" class="btn-record">属性更改记录</a>
                        </div>
                        <div class="pagination">
                            <a href="javaScript:void(0)" class="pagination-prev">上一条</a>
                            <a href="javaScript:void(0)" class="pagination-next">下一条</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
