<div class="table-diff">
    <div class="table-diff-content">

        <div class="table-diff-header">
            <div class="table-diff-left">
                <div class="table-diff-bar">
                    <a href="#slicesStandardData" class="showThan">比对</a>|
                    <!--<a href="javascript:void(0)" class="showDetail">查看详情</a>|-->
                    <a href="javascript:void(0)" class="cancel-than">取消比对</a>
                </div>
                <div class="table-diff-header">
                    <table width="100%" cellspacing="0" border="0">
                        <thead>
                            {{#each tLeftHead}}
                                <th><div>{{this}}</div></th>
                            {{/each}}
                        </thead>
                    </table>
                </div>
                <div class="table-diff-data">
                    <div class="table-diff-data-content">
                        <table width="100%" cellspacing="0" border="0">
                            <tbody>
                            {{#each ydata}}
                                <tr data-id={{id}} data-name="{{drugNameFormat}}">
                                    <td  title="{{id}}"><div>{{id}}</div></td>
                                    <td  title="{{drugName}}"><div>{{drugName}}</div></td>
                                    <td title="{{spec}}{{manufacturerName}}"><div>{{spec}}{{manufacturerName}}</div></td>
                                    <td class="convertRatio"><div><input value={{convertRatio}}></div></td>
                                    <td class="icon-triangle ymzzy" data-minUseUnit="{{minUseUnit}}" title="{{unitName}}"><div>{{unitName}}</div></td>
                                    <td class="icon-triangle table-diff-data-td-select"><div>{{{isStopOrNot isStop 'isStop'}}}</div></td>
                                    <td title="{{timeFn createDate}}"><div>{{timeFn createDate}}</div></td>
                                </tr>
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="table-diff-right">
                <div class="table-diff-right-all active">
                    <div class="table-diff-header">
                        <div class="table-diff-header-content">
                            <table cellspacing="0" border="0">
                                <thead>
                                <tr>
                                    {{#each tRightHead}}
                                        <th><div>{{this}}</div></th>
                                    {{/each}}
                                </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                    <div class="table-diff-data">
                        <table cellspacing="0" border="0">
                            <tbody>
                            {{#each ydata}}
                                <tr>
                                    <td title="{{codePro}}"><div>{{codePro}}</div></td>
                                    <td class="icon-triangle table-diff-data-td-select">
                                        <div class="value showValue">{{ypTypeSwitch ypType}}</div>
                                        <div class="select">
                                            <select  class="upSelect" data-name="ypType">
                                                <option value="0" {{isTrue ypType "0"}}>西药</option>
                                                <option value="1" {{isTrue ypType "1"}}>中成药</option>
                                                <option value="2" {{isTrue ypType "2"}}>中药饮片</option>
                                                <option value="3" {{isTrue ypType "3"}}>医院制剂</option>
                                                <option value="4" {{isTrue ypType "4"}}>卫生材料</option>
                                            </select>
                                        </div>
                                    </td>
                                    <td title="{{pzName}}"><div>{{pzName}}</div></td>
                                    <td title="{{proName}}"><div>{{proName}}</div></td>
                                    <td title="{{pxName}}"><div>{{pxName}}</div></td>
                                    <td class="icon-triangle table-diff-data-td-select" title="{{flag4Switch flag4}}">
                                        <div class="value showValue">{{flag4Switch flag4}}</div>
                                        <div class="select">
                                            <select  class="upSelect" data-name="flag4">
                                                <option value="0" {{isTrue flag4 "0"}}>否</option>
                                                <option value="1" {{isTrue flag4 "1"}}>国家</option>
                                                <option value="2" {{isTrue flag4 "2"}}>省基</option>
                                                <option value="3" {{isTrue flag4 "3"}}>地基</option>
                                            </select>
                                        </div>
                                    </td>
                                    <td class="icon-triangle table-diff-data-td-select" title="{{yblbSwitch yblb}}">
                                        <div class="value showValue">{{yblbSwitch yblb}}</div>
                                        <div class="select">
                                            <select  class="upSelect" data-name="yblb">
                                                <option value="0" {{isTrue yblb "0"}}>否</option>
                                                <option value="1" {{isTrue yblb "1"}}>国家</option>
                                                <option value="2" {{isTrue yblb "2"}}>省</option>
                                                <option value="3" {{isTrue yblb "3"}}>地市</option>
                                            </select>
                                        </div>
                                    </td>
                                    <td class="icon-triangle table-diff-data-td-select" title="{{ybbh}}">
                                        <div><input class="upInput" value="{{ybbh}}" data-name="ybbh"></div>
                                    </td>
                                </tr>
                            {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="table-diff-right-single">
                    <h2 class="table-diff-single-title">标准数据</h2>
                    <div class="table-diff-single-content">
                        <div class="table-details-content-box">
                            <!--标准数据详情-->
                        </div>
                        <div class="tool">
                            <a href="javaScript:void(0)" class="btn-toggle"><i class="icon-arrow-left"></i>切换</a>
                            <a href="javaScript:void(0)" class="btn btn-cancel">取消比对</a>
                            <a href="javaScript:void(0)" class="btn-record">属性更改记录</a>
                        </div>
                        <div class="pagination">
                            <a href="javaScript:void(0)" class="pagination-prev">上一条</a>
                            <a href="javaScript:void(0)" class="pagination-next">下一条</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="loading-wrap">
            <div class="loading">
                <img src="./images/loading.gif" class="loading-img">
            </div>
        </div>
    </div>
</div>
